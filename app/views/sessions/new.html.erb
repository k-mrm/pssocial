<h2>login</h2>
<%= flash[:alert] %>
<p>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Log in", class: "btn btn-primary" %>
  <% end %>
</p>
<p>
New to pssocial?
<%= link_to 'signup', signup_path %>
</p>
